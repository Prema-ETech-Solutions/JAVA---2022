package base.oop3;
//now we are creating a attributes and some data function in Second_7 class
public class Second_7 
{
	 int a = 10; // Attributes
	 Second_7 Obj = new Second_7();
	public static void sum()//data function
	{
		System.out.println("This is Second_7 Class ");
		System.out.println("SUM OF a+a = "+(Obj.a+Obj.a));
		//now go to Inheritance_7.java 
	}
	public void min()//data function
	{
		System.out.println("This is Second_7 Class ");
		System.out.println("SUM OF a+a = "+(a-a));
		//now go to Inheritance_7.java 
	}
	
}
